{
    "name": "spatie/laravel-db-snapshots",
    "description": "Quickly dump and load databases",
    "keywords": [
        "spatie",
        "laravel-db-snapshots"
    ],
    "homepage": "https://github.com/spatie/laravel-db-snapshots",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "league/flysystem": "^3.0",
        "spatie/db-dumper": "^3.3",
        "spatie/laravel-package-tools": "^1.6",
        "spatie/temporary-directory": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^8.22.3|^9.0.4|^10.0",
        "pestphp/pest-plugin-laravel": "^2.4|^3.1",
        "phpunit/phpunit": "^10.5|^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\DbSnapshots\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\DbSnapshots\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\DbSnapshots\\DbSnapshotsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
