{
    "name": "spatie/laravel-db-snapshots",
    "description": "Quickly dump and load databases",
    "keywords": [
        "spatie",
        "laravel-db-snapshots"
    ],
    "homepage": "https://github.com/spatie/laravel-db-snapshots",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.4.16|~5.5.x-dev|~5.5.x-dev",
        "league/flysystem": "^1.0.27",
        "spatie/db-dumper": "^2.7",
        "spatie/laravel-migrate-fresh": "^1.4.1",
        "spatie/temporary-directory": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.8",
        "orchestra/testbench": "^3.4.5|~3.5.x-dev|~3.5.x-dev",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\DbSnapshots\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\DbSnapshots\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\DbSnapshots\\DbSnapshotsServiceProvider"
            ]
        }
    }
}

